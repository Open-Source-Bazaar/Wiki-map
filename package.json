    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{html,md,less,json,yml,js,ts,tsx}": "prettier --write"
    },
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "prepare": "husky install",
        "test": "lint-staged",
        "start": "parcel src/index.html --open",
